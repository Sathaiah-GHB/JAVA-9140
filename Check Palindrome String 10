import java.io.*;
import java.util.*;

public class Solution {

    public static void main(String[] args) {
        Scanner s=new Scanner(System.in);
        String S=s.nextLine();
        boolean isPalindrome=true;
        int l=0;
        int r=S.length()-1;
        while(l<r){
            if(S.charAt(l)!=S.charAt(r)){
                isPalindrome=false;
            }
            l++;
            r--;
        }
       if(isPalindrome){
            System.out.println("Palindrome");
        }
        else{
            System.out.println("Not Palindrome");
        }
        
    
    }
}
