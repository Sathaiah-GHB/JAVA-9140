import java.io.*;
import java.util.*;

public class Solution {

    public static void main(String[] args) {
        Scanner s=new Scanner(System.in);
        String b=s.nextLine();
        int c=0;
        for(int i=0;i<b.length();i++){
            char p=b.charAt(i);
            if(p=='a'||p=='e'||p=='i'||p=='o'||p=='u'||p=='A'||p=='E'||p=='I'||p=='O'||p=='U'){
                c++;
            }
        }
        System.out.println(c);
    }
}
