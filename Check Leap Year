import java.io.*;
import java.util.*;

public class Solution {

    public static void main(String[] args) {
        int l;
        Scanner s=new Scanner(System.in);
        l=s.nextInt();
        if(l%4==0){
            System.out.println("Leap Year");
        }
        else{
            System.out.println("Not Leap Year");
        }
    }
}
