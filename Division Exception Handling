import java.io.*;
import java.util.*;

public class Solution {
    public static void main(String[] args) {
        Scanner s=new Scanner(System.in);
        try {
            int a=s.nextInt();
            int b=s.nextInt();
            int c=a/b;
            System.out.println(c);
            
        }
        catch(ArithmeticException c){
            System.out.println("Division by zero is not allowed");
}
    }
}
