import java.io.*;
import java.util.*;

public class Solution {

    public static void main(String[] args) {
        int n;
        Scanner s=new Scanner(System.in);
        do{
            n=s.nextInt();
            if (n==0){
                break;
            }
            System.out.println(n);
          
        }while(n!=0);
        
    }
}
