import java.io.*;
import java.util.*;

public class Solution {

    public static void main(String[] args) {
        Scanner s=new Scanner(System.in);
        char c;
        c=s.next().charAt(0);
        switch(c){
            case 'a':
            
            case 'e':
    
            case 'i':
            
            case 'o':
            
            case 'u':
                    {
            System.out.println("Vowel");
                break;
            }
            
            default:
            {
            System.out.println("Consonant");
                break;
            }
        }
        
    }
}
