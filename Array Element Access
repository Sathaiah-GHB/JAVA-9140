import java.io.*;
import java.util.*;

public class Solution {

    public static void main(String[] args) {
        Scanner s=new Scanner(System.in);
        try{
        int n=s.nextInt();
        int a[]=new int[n];
        for(int i=0;i<n;i++){
            a[i]=s.nextInt();
        }
        int c=s.nextInt();
        System.out.println(a[c]);
    }
        catch(ArrayIndexOutOfBoundsException k){
            System.out.println("Invalid Index");
        }
}
}
