import java.io.*;
import java.util.*;

public class Solution {
    public static void main(String[] args) {
        Scanner s=new Scanner(System.in);
        try {
            String n=s.nextLine();
            System.out.println(n.length());
            
        }
        catch(NoSuchElementException c){
            System.out.println("Null value found");
}
        /*String n=s.nextLine();
        System.out.println(n.length());*/
    }
}
