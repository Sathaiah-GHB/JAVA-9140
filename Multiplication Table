import java.io.*;
import java.util.*;

public class Solution {

    public static void main(String[] args) {
          int a;
        Scanner s=new Scanner(System.in);
        a=s.nextInt();
        for(int i=1;i<=10;i++){
            System.out.print(i+"*"+a+"=");
            System.out.println(i*a);
        }
    }
}
