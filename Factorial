import java.io.*;
import java.util.*;

public class Solution {

    public static void main(String[] args) {
        int a;
        int f=1;
        Scanner s=new Scanner(System.in);
        a=s.nextInt();
        for(int i=1;i<=a;i++){
            f=f*i;
        }
        System.out.println(f);
        
        }
}
