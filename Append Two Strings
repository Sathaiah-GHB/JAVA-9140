import java.io.*;
import java.util.*;

public class Solution {

    public static void main(String[] args) {
        Scanner sc=new Scanner(System.in);
        String t1=sc.nextLine();
        String t2=sc.nextLine();
        StringBuffer b=new StringBuffer(t1);
        b.append(t2);
        System.out.println(b);     
    }
}
