import java.io.*;
import java.util.*;

public class Solution {
    static int pow(int a ,int n){
        if(n==0){
            return 1;
        }
        if(n==1){
            return a;
        }
        else{
            return (a*pow(a,n-1));
        }
    }

    public static void main(String[] args) {
        Scanner s=new Scanner(System.in);
        int a=s.nextInt();
        int n=s.nextInt();
        System.out.println(pow(a,n));
    }
}
