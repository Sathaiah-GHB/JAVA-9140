import java.io.*;
import java.util.*;

public class Solution {

    public static void main(String[] args) {
        int d,n;
        int r=0;
        Scanner s=new Scanner(System.in);
        n=s.nextInt();  
        int t=n;
        while(n!=0){
            d=n%10;
            r=r*10+d;
            n=n/10;
        }
        if(t==r){
            System.out.println("Palindrome");
        }
        else{
            System.out.println("Not Palindrome");
        }
    }
}
